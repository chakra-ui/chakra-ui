{
  "name": "@chakra-ui/storybook-addon",
  "version": "0.0.0",
  "description": "The official Storybook Addon for Chakra UI",
  "main": "dist/preset.js",
  "types": "dist/types/index.d.ts",
  "typings": "dist/types/index.d.ts",
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "storybook": "npm run build && npm run build-storybook && start-storybook -p 6006 --no-manager-cache --quiet",
    "build-storybook": "build-storybook",
    "build": "tsc --build",
    "clean": "tsc --build --clean",
    "lint": "eslint .",
    "start": "tsc --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/chakra-ui/chakra-ui.git",
    "directory": "tooling/storybook-addon"
  },
  "keywords": [
    "storybook-addon",
    "chakra-ui",
    "storybook",
    "addons",
    "dark-mode"
  ],
  "author": "Andres Tuñón <andressp@ngxcoder.dev>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/chakra-ui/chakra-ui/issues"
  },
  "homepage": "https://github.com/chakra-ui/chakra-ui#readme",
  "devDependencies": {
    "@chakra-ui/react": "^1.4.1",
    "react": "^17.0.1"
  },
  "peerDependencies": {
    "@chakra-ui/react": "^1.0.0",
    "react": "16.x || 17.x"
  },
  "peerDependenciesMeta": {
    "react": {
      "optional": true
    },
    "react-dom": {
      "optional": true
    }
  },
  "publishConfig": {
    "access": "public"
  },
  "storybook": {
    "displayName": "Chakra UI",
    "supportedFrameworks": [
      "react"
    ],
    "unsupportedFrameworks": [
      "Vue",
      "Angular",
      "Svelte"
    ],
    "icon": "https://raw.githubusercontent.com/chakra-ui/chakra-ui/main/logo/logomark-colored.svg"
  }
}
