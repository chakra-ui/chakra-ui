    "cli-handle-unhandled": "^1.1.1",
    "cli-welcome": "^2.2.2",
    "commander": "^9.3.0",
    "lodash.throttle": "^4.1.1",
    "module-alias": "^2.2.2",
    "ora": "^5.3.0",
    "prettier": "^2.7.1",
    "regenerator-runtime": "^0.13.7",
    "ts-node": "^10.7.0",
    "tsconfig-paths": "^4.0.0",
    "update-notifier": "^5.0.1"
  },
  "devDependencies": {
    "@types/lodash.throttle": "^4.1.7",
    "@types/module-alias": "^2.0.1",
    "@types/ora": "^3.2.0",
    "@types/update-notifier": "5.0.0",
    "cross-env": "7.0.3"
