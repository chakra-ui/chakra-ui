{
  "name": "@chakra-ui/cli",
  "version": "2.4.1",
  "description": "Generate theme typings for autocomplete",
  "keywords": [
    "theme",
    "theming",
    "typings",
    "generator",
    "chakra ui",
    "ui mode",
    "ui"
  ],
  "author": "Tim Kolberger <tim@kolberger.eu>",
  "homepage": "https://github.com/chakra-ui/chakra-ui#readme",
  "license": "MIT",
  "main": "dist/cjs/index.cjs",
  "sideEffects": false,
  "files": [
    "dist",
    "bin"
  ],
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/chakra-ui/chakra-ui.git"
  },
  "bugs": {
    "url": "https://github.com/chakra-ui/chakra-ui/issues"
  },
  "bin": {
    "chakra-cli": "bin/index.js"
  },
  "scripts": {
    "clean": "rimraf dist .turbo",
    "build:fast": "tsup src --format cjs --shims",
    "build": "tsup src --format cjs --shims",
    "dev": "pnpm build:fast --watch",
    "typecheck": " tsc --noEmit"
  },
  "dependencies": {
    "bundle-n-require": "^1.0.1",
    "chokidar": "^3.5.3",
    "cli-check-node": "^1.3.4",
    "cli-handle-unhandled": "^1.1.1",
    "cli-welcome": "^2.2.2",
    "commander": "^11.0.0",
    "prettier": "^3.0.2"
  },
  "devDependencies": {
    "@types/ora": "^3.2.0",
    "@types/update-notifier": "6.0.4",
    "ora": "^7.0.1",
    "update-notifier": "^6.0.2"
  },
  "exports": {
    ".": {
      "source": "./src/index.mts",
      "import": {
        "types": "./dist/types/index.d.mts",
        "default": "./dist/esm/index.mjs"
      },
      "require": {
        "types": "./dist/types/index.d.ts",
        "default": "./dist/cjs/index.cjs"
      }
    },
    "./command": {
      "source": "./src/command/index.ts",
      "import": {
        "types": "./dist/types/command/index.d.mts",
        "default": "./dist/esm/command/index.mjs"
      },
      "require": {
        "types": "./dist/types/command/index.d.ts",
        "default": "./dist/cjs/command/index.cjs"
      }
    },
    "./utils": {
      "source": "./src/utils/index.ts",
      "import": {
        "types": "./dist/types/utils/index.d.mts",
        "default": "./dist/esm/utils/index.mjs"
      },
      "require": {
        "types": "./dist/types/utils/index.d.ts",
        "default": "./dist/cjs/utils/index.cjs"
      }
    },
    "./package.json": "./package.json"
  },
  "module": "dist/esm/index.mjs",
  "types": "dist/types/index.d.ts"
}
