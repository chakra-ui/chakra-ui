{
  "Root": {
    "props": {
      "maxLines": {
        "type": "number | undefined",
        "defaultValue": "undefined",
        "isRequired": false,
        "description": "The maximum number of lines to show."
      },
      "defaultColorScheme": {
        "type": "CodeBlockColorScheme | undefined",
        "defaultValue": "dark",
        "isRequired": false,
        "description": "The fallback color scheme for the code block."
      },
      "copyTimeout": {
        "type": "number | undefined",
        "defaultValue": "1000",
        "isRequired": false,
        "description": "The timeout for the copy action."
      },
      "onCopy": {
        "type": "VoidFunction | undefined",
        "isRequired": false,
        "description": "The callback function that is called when the copy action is completed."
      },
      "code": {
        "type": "string",
        "isRequired": true,
        "description": "The code to highlight."
      },
      "language": {
        "type": "string | undefined",
        "isRequired": false,
        "description": "The language of the code."
      },
      "meta": {
        "type": "CodeBlockHighlightMeta | undefined",
        "isRequired": false,
        "description": "The meta data for the code."
      },
      "as": {
        "type": "React.ElementType",
        "isRequired": false,
        "description": "The underlying element to render."
      },
      "asChild": {
        "type": "boolean",
        "isRequired": false,
        "description": "Use the provided child element as the default rendered element, combining their props and behavior."
      },
      "unstyled": {
        "type": "boolean",
        "isRequired": false,
        "description": "Whether to remove the component's style."
      },
      "colorPalette": {
        "defaultValue": "gray",
        "type": "\"gray\" | \"red\" | \"orange\" | \"yellow\" | \"green\" | \"teal\" | \"blue\" | \"cyan\" | \"purple\" | \"pink\"",
        "isRequired": false,
        "description": "The color palette of the component"
      },
      "size": {
        "type": "\"sm\" | \"md\" | \"lg\"",
