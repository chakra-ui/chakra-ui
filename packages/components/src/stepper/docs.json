      "description": "The visual color appearance of the component"
    },
    "size": {
      "defaultValue": "md",
      "type": "\"xs\" | \"sm\" | \"md\" | \"lg\"",
      "required": false,
      "description": "The size of the Stepper"
    },
    "variant": {
      "type": "string",
      "required": false,
      "description": "The variant of the Stepper"
    }
  },
  "UseSteps": {
    "count": { "type": "number", "required": true },
