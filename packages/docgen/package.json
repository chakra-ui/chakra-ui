{
  "name": "@chakra-ui/docgen",
  "version": "1.0.0",
  "description": "Documentation generator for Chakra UI components",
  "keywords": [
    "chakra-ui",
    "docgen",
    "documentation",
    "generator"
  ],
  "type": "module",
  "main": "dist/esm/index.js",
  "module": "dist/esm/index.js",
  "types": "src/index.ts",
  "scripts": {
    "prepack": "tsx ../../scripts/conditions.ts",
    "postpack": "tsx ../../scripts/conditions.ts --restore",
    "clean": "rimraf dist .turbo",
    "build:fast": "tsx ../../scripts/build/main.ts",
    "build": "tsx ../../scripts/build/main.ts --dts",
    "dev": "pnpm build:fast --watch",
    "typecheck": " tsc --noEmit"
  },
  "author": "Tioluwani Kolawole",
  "license": "MIT",
  "dependencies": {
    "@chakra-ui/react": "workspace:*",
    "consola": "^3.4.2",
    "fs-extra": "11.3.0",
    "node-fetch": "3.3.2",
    "scule": "1.3.0"
  }
}
